{
  "name": "ss-2",
  "version": "0.3.3",
  "private": true,
  "license": "MIT",
  "author": "Q3 Labs",
  "workspaces": {
    "packages": [
      "packages/contracts",
      "packages/starkcade-web"
    ]
  },
  "scripts": {
    "chain": "yarn workspace @ss-2/contracts chain",
    "compile": "yarn workspace @ss-2/contracts compile",
    "deploy": "yarn workspace @ss-2/contracts deploy",
    "deploy:no-reset": "yarn workspace @ss-2/contracts deploy --no-reset",
    "format": "yarn workspace @ss-2/starkcade-web format && yarn workspace @ss-2/contracts format",
    "format:check": "yarn workspace @ss-2/starkcade-web format:check && yarn workspace @ss-2/contracts format:check",
    "next:check-types": "yarn workspace @ss-2/starkcade-web check-types",
    "next:lint": "yarn workspace @ss-2/starkcade-web lint",
    "prepare": "husky",
    "start": "yarn workspace @ss-2/starkcade-web dev",
    "test": "yarn workspace @ss-2/contracts test",
    "test:starkcade-web": "yarn workspace @ss-2/starkcade-web test",
    "vercel": "yarn workspace @ss-2/starkcade-web vercel",
    "vercel:yolo": "yarn workspace @ss-2/starkcade-web vercel:yolo",
    "verify": "yarn workspace @ss-2/contracts verify"
  },
  "dependencies": {
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "daisyui": "^4.7.3",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "prettier": "^3.2.5"
  }
}
